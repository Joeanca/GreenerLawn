<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.greenerlawn.greenerlawn">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/green"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/green_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainScreen"
            android:theme="@style/AppTheme"
            >

            <!--
             to make this the starting activity uncomment the intent filter and the content within,
             also comment it from any other classes which have the android.intent.action.MAIN
            -->

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TimePopUp"
            android:theme="@style/AppTheme.TimerPopup"></activity>
        <activity android:name=".ZoneSettings">

            <!--
             to make this the starting activity uncomment the intent filter and the content within,
             also comment it from any other classes which have the android.intent.action.MAIN
            -->


            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->
            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->

        </activity>
    </application>

</manifest>